function selectEventFromThisType(labelType) {
    var types_of_event = _environments_environment__WEBPACK_IMPORTED_MODULE_1__["data_0A10761"]["types_of_event"];
    var events = _environments_environment__WEBPACK_IMPORTED_MODULE_1__["data_0A10761"]["events"];
    d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]('#types-of-event li.selected')
        .attr('class', 'types-of-event')
        .style('color', '#EDEAD0');
    for (var idTypeEvent in types_of_event) {
        if (labelType == types_of_event[idTypeEvent]['label']) {
            var linked_events = types_of_event[idTypeEvent]['linked_events'];
            // Increase size of event rectangle from type selected
            for (var eventId of linked_events) {
                d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]('svg #' + eventId)
                    .attr("height", 30)
                    .attr("y", 420);
            }
            //decrease size of other event rectangle
            for (var event of events) {
                if (eventId != event["id"]) {
                    var id_event = event["id"];
                    d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]("g#g1 rect#" + id_event)
                        .attr("height", 10)
                        .attr("y", 440);
                }
            }
            break;
        }
    }
    d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]("#types-of-event #" + labelType)
        .attr('class', 'types-of-event selected')
        .style('color', '#FF775C');
}
