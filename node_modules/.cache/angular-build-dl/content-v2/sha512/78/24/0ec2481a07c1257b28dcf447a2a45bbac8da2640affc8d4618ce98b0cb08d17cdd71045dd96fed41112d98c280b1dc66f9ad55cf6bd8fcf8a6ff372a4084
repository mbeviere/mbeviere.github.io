function enhanceLinkedDocuments() {
    var stratums = _environments_environment__WEBPACK_IMPORTED_MODULE_1__["data_0A10761"]["stratums"];
    var id_stratum = null;
    try {
        id_stratum = d3__WEBPACK_IMPORTED_MODULE_0__["select"]("#left-column h2.selected").attr('id');
    }
    catch (_a) { }
    // remove styles from previous selected stratum
    d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]("#right-content .inner-circle").remove();
    d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]("#left-column h2")
        .style('color', '#EDEAD0');
    if (id_stratum != null) {
        // apply style to the selected stratum
        d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]("#left-column h2.selected")
            .style('color', '#FF775C');
        // enhance linked document with a white stroke
        var document_dot;
        var g;
        for (var index_stratum in stratums) {
            if (stratums[index_stratum]["id"] == id_stratum) {
                for (var index_document in stratums[index_stratum]["linked_documents"]) {
                    var id_document = stratums[index_stratum]["linked_documents"][index_document]["id"];
                    document_dot = d3__WEBPACK_IMPORTED_MODULE_0__["select"]("#right-content #" + id_document);
                    g = document_dot.select(function () { return this.parentNode; });
                    g.append('circle')
                        .attr('class', 'inner-circle')
                        .attr('id', 'inner-' + id_document)
                        .attr('cx', function () {
                        return d3__WEBPACK_IMPORTED_MODULE_0__["select"]("#right-content #" + id_document).attr('cx');
                    })
                        .attr('cy', function () {
                        return d3__WEBPACK_IMPORTED_MODULE_0__["select"]("#right-content #" + id_document).attr('cy');
                    })
                        .attr('r', function () {
                        return 4;
                    })
                        .attr('fill', '#EDEAD0')
                        .attr('stroke', '#EDEAD0')
                        .attr('stroke-width', '1.8')
                        .attr('stroke-dashoffset', '5')
                        .style('pointer-events', 'none');
                }
            }
        }
    }
}
