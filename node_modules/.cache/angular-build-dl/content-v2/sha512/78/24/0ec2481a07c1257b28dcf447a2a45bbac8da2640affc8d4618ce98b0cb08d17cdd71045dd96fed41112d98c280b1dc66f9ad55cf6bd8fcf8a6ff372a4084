function resetGraph() {
    //reset all document as not selected
    d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]('svg circle.selected')
        .attr('class', 'document-dot')
        .style('fill', '#3f4957')
        .style('stroke', '#EDEAD0')
        .attr('r', 10);
    d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]('#document-list li.selected')
        .attr('class', 'document-in-list')
        .style('color', '#EDEAD0');
    d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"](".link").style('display', 'none');
    // deselect any documents linked to a previous event
    d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]('svg circle.document-dot')
        .style('stroke', '#EDEAD0');
    d3__WEBPACK_IMPORTED_MODULE_0__["select"]("document-detail").style('display', 'none');
