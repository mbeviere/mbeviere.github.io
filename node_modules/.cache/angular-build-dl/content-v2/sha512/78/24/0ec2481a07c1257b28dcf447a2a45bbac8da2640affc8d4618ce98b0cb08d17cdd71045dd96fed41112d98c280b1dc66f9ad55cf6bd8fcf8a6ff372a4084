                .attr('class', 'document-dot')
                .style('fill', '#3f4957')
                .style('stroke', '#EDEAD0')
                .attr('r', 10);
        }
        d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]('svg circle.document-dot')
            .style('stroke', '#EDEAD0');
        //deselect any previous event  rectangle selected
        d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]('svg rect.selected')
