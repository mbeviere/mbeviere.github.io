    var id_linked_document;
    var document_dot;
    var linked_document_dot;
    var links = [];
    let idDocumentSelected;
    // if there is any circle already selected
    try {
        idDocumentSelected = d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"]('svg circle.selected').attr('id');
    }
    catch (_a) { }
    // Show documents details
    for (var index_document in documents) {
        if (documents[index_document]["id"] == idDocument) {
            if (documents[index_document]['date'] != null) {
                var dateYear = new Date(documents[index_document]['date']['timestamp'] * 1000).getFullYear();
                d3__WEBPACK_IMPORTED_MODULE_0__["select"]("div#document-detail td#date").text(String(dateYear));
            }
            else {
                d3__WEBPACK_IMPORTED_MODULE_0__["select"]("div#document-detail td#date").text("");
            }
            d3__WEBPACK_IMPORTED_MODULE_0__["select"]("div#document-detail h1").text(documents[index_document]["label"]);
            d3__WEBPACK_IMPORTED_MODULE_0__["select"]("div#document-detail td#authors").text(documents[index_document]["creator"]);
            d3__WEBPACK_IMPORTED_MODULE_0__["select"]("div#doc-part-2 header").selectAll("h2").remove();
            for (var index_stratum in documents[index_document]["linked_stratums"]) {
                d3__WEBPACK_IMPORTED_MODULE_0__["select"]("div#doc-part-2 header")
                    .append("h2")
                    .text(documents[index_document]["linked_stratums"][index_stratum])
                    .style('font-family', 'RobotoBold')
                    .style('font-size', '12px')
                    .style('font-weight', 'normal')
                    .style('background-color', '#E16952')
                    .style('color', '#EDEAD0')
                    .style('padding', '3px 9px')
                    .style('margin', '0 0 2px 0')
                    .style('border-radius', '2px');
            }
            d3__WEBPACK_IMPORTED_MODULE_0__["selectAll"](".link").style('display', 'none');
            // add links between documents
            for (var index_linked_document in documents[index_document]["linked_documents"]) {
